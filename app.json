{
  "expo": {
    "name": "ppark",
    "slug": "ppark",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ppark",
      "config": {
        "googleMapsApiKey": "AIzaSyDFviNwbJMBXOwDzTPWUxgCamIiAjrW2mo"
      }

    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png"
      },
      "package": "com.Ppark",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDFviNwbJMBXOwDzTPWUxgCamIiAjrW2mo"
        }}
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(Ppark) to use your location."
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    }

  }
}